{
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  
    "rewrites": [
      { "source": "/(.*)", 
      "destination": "/" 
      }
    ],
    "functions": {
      "api/clientes/**/*.js": {
        "maxDuration": 90
      }
    }
  }
